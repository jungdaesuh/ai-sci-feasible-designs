# Strict VMEC++ single-thread settings
OMP_NUM_THREADS=1
MKL_NUM_THREADS=1
OPENBLAS_NUM_THREADS=1

# AI Scientist provider/model overrides
# Source this file (e.g., `set -a; source .env`) before running ai_scientist tools
# to ensure the runner uses the desired OpenRouter tier.
MODEL_PROVIDER=openrouter
AI_SCIENTIST_INSTRUCT_MODEL="kwaipilot/kat-coder-pro:free-short-loop"
AI_SCIENTIST_THINKING_MODEL="kwaipilot/kat-coder-pro:free"
